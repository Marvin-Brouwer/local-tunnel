{
	"name": "@local-tunnel/cli",
	"version": "3.0.0-alpha.1",
	"description": "Command line interface for @local-tunnel/client",
	"main": "./dist/index.mjs",
	"bin": {
		"lf": "./lib/index.mjs",
		"localtunnel": "./lib/index.mjs",
		"local-tunnel": "./lib/index.mjs"
	},
	"exports": {
	  ".": {
		"npx": "./dist/index.mjs"
	  }
	},
	"scripts": {
		"lt:dev:8080": "pnpm lt http://localhost:8080 --remote-host localtunnel-dev --subdomain 8080 --print-requests",
		"lt:dev:8008": "pnpm lt http://localhost:8008 --remote-host localtunnel-dev --subdomain 8008 --print-requests",
		"lt:dev:astro": "pnpm lt https://localhost:4321 -skip-cert-val --remote-host localtunnel-dev --subdomain astro --print-requests",
		"lt:ext:8080": "pnpm lt http://localhost:8080 --subdomain 8080 --print-requests",
		"lt:ext:8008": "pnpm lt http://localhost:8008 --subdomain 8008 --print-requests",
		"lt:ext:astro": "pnpm lt https://localhost:4321 -skip-cert-val --subdomain astro --print-requests",
		"lt": "node ./lib/index.mjs",
		"build": "vite build",
		"build:production": "vite optimize && vite build --mode production",
		"lint": "pnpm eslint -c ../../.eslintrc.js ./src/** ./plugins/** ./vite.config.mts",
		"lint:fix": "pnpm lint --fix",
		"watch": "pnpm build --mode development --watch",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [],
	"author": "Marvin Brouwer",
	"license": "MIT",
	"dependencies": {
		"@local-tunnel/client": "workspace:*",
		"chalk": "^5.3.0",
		"commander": "^12.0.0",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@types/node": "^20.11.22",
		"eslint": "^8.57.0",
		"rollup": "^4.12.0",
		"vite": "^5.1.4",
		"vite-plugin-linter": "^2.1.1",
		"vite-plugin-node-polyfills": "^0.21.0",
		"debug": "^4.3.4"
	}
}